    <item>
      <title>Comme une fée</title>
      <link>http://arthurmasson.xyz/fr/comme-une-fee/</link>
      <pubDate>Thu, 30 Jul 2009 00:00:53 +0000</pubDate>
      <guid>http://arthurmasson.xyz/fr/comme-une-fee/</guid>
